{
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/build",
  "rewrites": [
    {
      "source": "/api/auth/(.*)",
      "destination": "/api/auth.js"
    },
    {
      "source": "/api/babies(.*)",
      "destination": "/api/babies.js"
    },
    {
      "source": "/api/sleep(.*)",
      "destination": "/api/sleep.js"
    }
  ]
}