{
  "rewrites": [
    {
      "source": "/api/test",
      "destination": "/api/test.js"
    },
    {
      "source": "/api/simple",
      "destination": "/api/simple.js"
    },
    {
      "source": "/api/minimal/(.*)",
      "destination": "/api/minimal.js"
    },
    {
      "source": "/api/auth/(.*)",
      "destination": "/api/auth.js"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    }
  ]
}